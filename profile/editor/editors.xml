<editors>
    <editor id="status">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="temperature">
        <range uom="4" min="-50" max="65" prec="1" />
        <range uom="17" min="-50" max="150" prec="1" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="co2">
        <range uom="54" min="0" max="1000" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="humidity">
        <range uom="51" min="0" max="100" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="noise">
        <range uom="12" min="0" max="1000" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="pressure">
        <range uom="117" min="0" max="10000" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="trend">
        <range uom="25" subset="0-2,99" nls="EN_TREND" />
    </editor>

    <editor id="t_timestamp">
        <range uom="44" min="0" max="525600" prec="2" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>


    <editor id="percent">
        <range uom="51" min="0" max="100" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="rain">
        <range uom="82" min="0" max="100000" prec="1" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="w_strength">
        <range uom="48" min="0" max="1000" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="w_angle">
        <range uom="76" min="0" max="360" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

    <editor id="battery">
        <range uom="25" subset="0-6,99" nls="battery" />
    </editor>

    <editor id="rf_status">
        <range uom="25" subset="0-2,99" nls="rf_status" />
    </editor>

    <editor id="rf_strength">
        <range uom="131"  min="-174" max="30" prec="0" />
        <range uom="25" subset="0,99" nls="error" />
    </editor>

</editors>
